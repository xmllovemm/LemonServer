<?xml version="1.0" encoding="utf-8"?>
<root>
  
  <!--program init-->
  <program>
    <!--root directory-->
    <workdir>/home/xml/workspace/test/icsserver/bin</workdir>
    
    <!--be a daemon process: 0-no,1-yes-->
    <daemon>0</daemon>
    
    <!--work on ics center mode: 0-no,1-yes-->
    <icscenter>1</icscenter>
    
    <!--number of work thread-->
    <workerthread>4</workerthread>

    <!--size of each chunk in memory pool-->
    <chunksize>126</chunksize>
    
    <!--count of chunk in memory pool-->
    <chunkcount>50</chunkcount>
  </program>

  <!--address: ip:port-->
  <centeraddr>
    <terminal>0.0.0.0:9999</terminal>
    <web>0.0.0.0:9998</web>
    <msgpush>192.168.50.133:8886</msgpush>
  </centeraddr>

  <proxyraddr>
    <terminal>0.0.0.0:8888</terminal>
    <center>0.0.0.0:8886</center>
  </proxyraddr>


  <!--ics protocol-->
  <protocol>
    <heartbeat>15</heartbeat>
    <onlineIP>192.168.50.112</onlineIP>
    <onlinePort>9998</onlinePort>
  </protocol>


  <!--log file-->
  <log>
    <configfile>log4cplus.properties</configfile>
  </log>
 
  <!--database info-->
  <database>
    <username>commuser</username>
    <password>datang</password>
    <dsn>mysql</dsn>
    <count>4</count>
  </database>
  
</root>
