<?xml version="1.0" encoding="utf-8"?>
<root>
  
  <!--program init-->
  <program>
    <!--root directory-->
    <workdir>/home/xml/workspace/test/icsserver/bin</workdir>
    <!--be a daemon process-->
    <daemon>0</daemon>
    <!--number of work thread-->
    <workerthread>4</workerthread>
    <chunksize>126</chunksize>
    <chunkcount>50</chunkcount>
  </program>

  <!--listen address: ip:port-->
  <listen>
    <addr>0.0.0.0:9999</addr>
  </listen>

  <!--ics protocol-->
  <protocol>
    <heartbeat>40</heartbeat>
    <onlineIP>127.0.0.1</onlineIP>
    <onlinePort>9999</onlinePort>
  </protocol>

  <pushmsg>
    <serverip>192.168.50.133</serverip>
    <serverport>9900</serverport>
  </pushmsg>
 
  <!--log file-->
  <log>
    <configfile>log4cplus.properties</configfile>
  </log>
 
  <!--database info-->
  <database>
    <username>commuser</username>
    <password>datang</password>
    <dsn>mysql</dsn>
    <count>4</count>
  </database>
  
</root>
